{
  "name": "nidle-nestjs",
  "version": "0.1.0",
  "description": "refactor nidle using nestjs.",
  "author": "barneyzhao <zxzswa@163.com>",
  "private": true,
  "license": "ISC",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@bull-board/api": "^5.7.1",
    "@bull-board/express": "^5.7.1",
    "@bull-board/nestjs": "^5.7.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.0.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.3",
    "@nestjs/platform-express": "^10.0.3",
    "@nestjs/swagger": "^7.0.8",
    "@nestjs/typeorm": "^10.0.0",
    "axios": "^1.4.0",
    "bull": "^4.10.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "connect-redis": "^7.1.0",
    "dayjs": "^1.11.9",
    "express-session": "^1.17.3",
    "extend": "^3.0.2",
    "hbs": "^4.2.0",
    "lodash": "^4.17.21",
    "mysql2": "^3.5.0",
    "nidle": "^0.1.8",
    "nidle-chain": "^0.1.8",
    "nidle-plugin-build": "^0.1.8",
    "nidle-plugin-clone": "^0.1.8",
    "nidle-plugin-escheck": "^0.1.8",
    "nidle-plugin-eslint": "^0.1.8",
    "nidle-plugin-imagemin": "^0.1.8",
    "nidle-plugin-install": "^0.1.8",
    "nidle-plugin-manifest": "^0.1.8",
    "nidle-plugin-merge": "^0.1.8",
    "nidle-plugin-nvm": "^0.1.8",
    "nidle-plugin-scp": "^0.1.8",
    "nidle-plugin-start": "^0.1.8",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "require-from-string": "^2.0.2",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^4.6.3",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.5",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.3",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/extend": "^3.0.1",
    "@types/hbs": "^4.0.1",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.2",
    "@types/require-from-string": "^1.2.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yanuoda/nidle.git"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
